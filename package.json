{
  "name": "react-hirc",
  "version": "1.0.4",
  "description": "React Hooks (Immer + Reducer + Context)",
  "repository": "git@github.com:miZyind/react-hirc.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/react-hirc#readme",
  "bugs": {
    "url": "https://github.com/miZyind/react-hirc/issues"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project ."
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "hooks",
    "immer",
    "reducer",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "immer": "^6.0.5",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "eslint": "^7.0.0",
    "eslint-config-mizyind": "^1.0.18",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "typescript": "^3.9.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/all",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended",
      "mizyind"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
