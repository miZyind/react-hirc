{
  "name": "react-hirc",
  "version": "1.0.8",
  "description": "React Hooks (Immer + Reducer + Context)",
  "repository": "git@github.com:miZyind/react-hirc.git",
  "author": "miZyind <mizyind@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/miZyind/react-hirc#readme",
  "bugs": {
    "url": "https://github.com/miZyind/react-hirc/issues"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc",
    "lint": "eslint \"*/**/*.ts\" --max-warnings 0"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "keywords": [
    "react",
    "hooks",
    "immer",
    "reducer",
    "redux",
    "typescript"
  ],
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "immer": "^7.0.9",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.49",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mizyind": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "es6": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json"
    },
    "plugins": [
      "@typescript-eslint",
      "prettier",
      "mizyind"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/all",
      "prettier/@typescript-eslint",
      "plugin:prettier/recommended",
      "plugin:mizyind/recommended"
    ],
    "rules": {
      "@typescript-eslint/comma-dangle": "off"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
}
